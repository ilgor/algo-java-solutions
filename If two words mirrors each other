public class BitWiseCheck
{
    public static int checkIfMirror(String a, String b)
    {
        return a.length() == b.length() && (a + b).contains(a);
    }

    public static void main(String[] args)
    {
        String a = "abc";
        String b = "cab";

        System.out.println(checkIfMirror(a, b));
    }
}
